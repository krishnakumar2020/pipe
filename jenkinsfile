node {
      stage('code clone') {
         git credentialsId: 'loginapp', url: 'https://github.com/krishnakumar2020/dell.git'
         }
	  stage('Clean') {
		sh 'mvn clean'
		 }
	  stage('Compile') {
		sh 'mvn compile'
		 }
	  stage('test') {
		sh 'mvn test'
		 }
	  stage('Packages') {
		sh 'mvn package'
		 }
	  stage('Deploy') {
		sh 'mvn deploy'
		 }
	}